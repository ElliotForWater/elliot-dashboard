{
  "manifest_version": 2,
  "name": "ElliotForWater",
  "version": "2.1.0",
  "description": "This extension sets your search engine to ElliotForWater to provide clean drinking water to countries in need with every search.",
  "icons": {
    "128": "icons/128.png",
    "16": "icons/16.png",
    "24": "icons/24.png",
    "32": "icons/32.png",
    "48": "icons/48.png"
  },
  "browser_action": {
    "browser_style": true,
    "default_icon": {
      "128": "icons/128.png",
      "16": "icons/16.png",
      "24": "icons/24.png",
      "32": "icons/32.png",
      "48": "icons/48.png"
    },
    "default_title": "The search engine to provide clean drinking water with your searches"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "encoding": "UTF-8",
      "favicon_url": "https://elliotforwater.com/images/metas/FAVICON-Elliot.png",
      "is_default": true,
      "keyword": "elliotforwater",
      "name": "Elliot For Water",
      "search_url": "https://elliotforwater.com/search?query={searchTerms}&type=web&method=topbar"
    }
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage",
    "tabs",
    "cookies",
    "*://*.elliotforwater.com/",
    "*://*.finditnowonline.com/"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://localhost:3000/*",
        "*://elliotforwater.com/*"
      ],
      "all_frames": true,
      "js": [
        "content-scripts.js"
      ],
      "run_at": "document_start"
    }
  ]
}