{
  "name": "ElliotForWater",
  "version": "3.0",
  "manifest_version": 2,
  "description": "This extension sets Elliot for Water as your default search engine so that, you can help donate clean water every time you search.",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": {
      "128": "icons/128.png",
      "16": "icons/16.png",
      "24": "icons/24.png",
      "32": "icons/32.png",
      "48": "icons/48.png"
    }
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "encoding": "UTF-8",
      "favicon_url": "https://elliotforwater.com/images/metas/FAVICON-Elliot.png",
      "is_default": true,
      "keyword": "elliotforwater",
      "name": "Elliot For Water",
      "search_url": "https://elliotforwater.com/search?query={searchTerms}&type=web&method=topbar"
    }
  },
  "chrome_url_overrides": {
    "newtab": "newtab/index.html"
  },
  "permissions": [
    "cookies",
    "*://elliotforwater.com/*",
    "storage"
  ],
  "externally_connectable": {
    "matches": [
      "*://elliotforwater.com/*",
      "*://www.elliotforwater.com/*"
    ]
  },
  "content_security_policy": "script-src 'self' https://suggest.finditnowonline.com; object-src 'self'",
  "homepage_url": "https://elliotforwater.com/",
  "icons": {
    "128": "icons/128.png",
    "16": "icons/16.png",
    "24": "icons/24.png",
    "32": "icons/32.png",
    "48": "icons/48.png"
  },
  "key": "",
  "short_name": "ElliotForWater",
  "update_url": "https://clients2.google.com/service/update2/crx",
}